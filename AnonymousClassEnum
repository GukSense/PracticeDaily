 public void setBookGenre(BookGenre bookGenre) {
		this.bookGenre = bookGenre;
	}
  
  
  public String getString() {
			String returnToHangul;
			
			switch (country) {
			case KOREA:
				returnToHangul = "대한민국";
				break;
			case USA:
				returnToHangul = "미국";
				break;
			case UK:
				returnToHangul = "영국";
				break;
			default:
				returnToHangul = "";
			}
			return returnToHangul;
		}
